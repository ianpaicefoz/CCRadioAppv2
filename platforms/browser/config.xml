<?xml version='1.0' encoding='utf-8'?>
<widget id="com.arbr-dev.ccradioapp" version="2.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>CCRadioApp</name>
    <description>
        Aplicaci√≥n para escuchar CCRadio en tu Smartphone o dispositivo mobil.
    </description>
    <author email="arbr.dev@gmail.com" href="http://www.ccradio.es">
        ARBR Proxectos.
    </author>
    <content src="index.html" />
    <preference name="android-minSdkVersion" />
    <preference name="android-targetSdkVersion" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="phonegap-version" value="cli-9.0.0" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
